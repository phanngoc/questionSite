<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.story %></td>
    <td><img src="<%= asset_path(user.avatar) %>" class="img_25" /></td>
    <td><%= user.created_at %></td>
    <td><%= user.role %></td>
    <td>
      <a href="<%= edit_admin_user_path(user.id) %>">
        <i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>
    </td>
  </tr>
<% end %>
